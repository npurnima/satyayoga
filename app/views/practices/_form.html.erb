<%= form_for(@practice) do |f| %>
  <% if @practice.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@practice.errors.count, "error") %> prohibited this practice from being saved:</h2>

      <ul>
      <% @practice.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>


    <%= f.label :date %>
    <%= f.text_field :date ,:class=>'datepicker'%>


    <%= f.label :course_id %>
   <div class="course-select"> <%= f.collection_select(:course_id,@courses,:id,:name) %> </div> <br/>
    </br>  <br/>
    <%= f.label :course_from_number %>
    <%= f.number_field :course_from_number %>
     </br>  <br/>
    <%= f.label :course_to_number %>
    <%= f.number_field :course_to_number %>
     <br/>
    <%= f.label :guide_id %>
    <%= f.select :guide_id,options_for_select(Userrole::Userrole.find_guides_names) %>  <br/>
    <br/>
    <%= f.label :guide_experience %>
    <%= f.text_field :experience %>

  <div class="actions">
    <%= f.submit  %>
  </div>
<% end %>
